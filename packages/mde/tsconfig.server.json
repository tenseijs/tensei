{
    "compilerOptions": {
        "target": "ES2017",
        "module": "commonjs",
        "declaration": true,
        "outDir": "./build/",
        "strict": true,
        "baseUrl": "./server",
        "esModuleInterop": true,
        "skipLibCheck": true,
        "forceConsistentCasingInFileNames": true,
        "types": ["@types/jest"]
    },
    "exclude": ["__tests__", "build", "src/components"]
}
