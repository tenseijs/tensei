{
  "name": "@tensei/cms",
  "version": "0.10.0",
  "main": "./build/index.js",
  "license": "MIT",
  "types": "./build/index.d.ts",
  "files": [
    "build/",
    "default-assets/"
  ],
  "scripts": {
    "prettier": "prettier --write './**/*.{js,json,ts,tsx,css}'",
    "dev-server": "tsc --project tsconfig.server.json --watch",
    "dev": "concurrently 'npm:dev-client' 'npm:dev-server'",
    "build-server": "tsc --project tsconfig.server.json && yarn copy-files",
    "copy-files": "copyfiles -u 1 plugin/template/index.mustache build/",
    "dev-client": "webpack --config=node_modules/laravel-mix/setup/webpack.config.js --stats=errors-only --watch",
    "build-client": "cross-env NODE_ENV=production webpack --config=node_modules/laravel-mix/setup/webpack.config.js",
    "build": "yarn build-server && yarn build-client"
  },
  "devDependencies": {
    "@babel/plugin-proposal-class-properties": "^7.12.1",
    "@babel/plugin-proposal-object-rest-spread": "^7.12.1",
    "@babel/preset-react": "^7.12.7",
    "@babel/preset-typescript": "^7.12.7",
    "@tensei/components": "^0.10.0",
    "@types/axios": "^0.14.0",
    "@types/classnames": "^2.2.11",
    "@types/passport": "^1.0.7",
    "@types/qs": "^6.9.5",
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "@types/react-router-dom": "^5.1.6",
    "@types/speakingurl": "^13.0.2",
    "@types/styled-components": "^5.1.15",
    "@types/throttle-debounce": "^2.1.0",
    "autoprefixer": "^10.2.5",
    "axios": "^0.21.1",
    "classnames": "^2.2.6",
    "copyfiles": "^2.4.1",
    "cross-env": "^7.0.3",
    "esbuild": "^0.13.4",
    "laravel-mix": "^6.0.34",
    "postcss-inline-svg": "^5.0.0",
    "qs": "^6.9.4",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-router-dom": "^5.2.0",
    "speakingurl": "^14.0.1",
    "throttle-debounce": "^3.0.1",
    "ts-loader": "^8.0.11",
    "typescript": "^4.2.4",
    "webpack-bundle-analyzer": "^4.5.0",
    "webpack-require-from": "^1.8.6"
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {
    "@tensei/core": "^0.10.0",
    "@tensei/eui": "1.0.3",
    "@tensei/mail": "^0.10.0",
    "@types/csurf": "^1.11.0",
    "@types/express-session": "^1.17.3",
    "csurf": "^1.11.0",
    "express-session": "^1.17.1",
    "express-session-mikro-orm": "^0.10.0",
    "moment": "^2.29.1",
    "passport": "^0.4.1",
    "passport-local": "^1.0.0",
    "styled-components": "^5.3.3",
    "throttle-debounce": "^3.0.1",
    "zustand": "^3.6.5"
  }
}
